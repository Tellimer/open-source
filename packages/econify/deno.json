{
  "name": "@tellimer/econify",
  "version": "0.3.3",
  "tasks": {
    "dev": "deno run --watch src/helpers/sample_usage.ts",
    "test": "deno test",
    "test:cov": "deno test --allow-read --coverage=coverage",
    "cov:lcov": "deno coverage coverage --lcov > coverage/lcov.info",
    "cov:check": "deno run --allow-read scripts/check_coverage.ts coverage/lcov.info 100",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "update-code3": "deno run --allow-read --allow-write src/helpers/update_code3.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "xstate": "npm:xstate@^5.20.2"
  },
  "description": "Economic data processing toolkit: classify indicators, parse units, and normalize values across currency, magnitude, and time with quality scoring, FX (live/historical), inflation, seasonal adjustment, aggregations, and an XState-powered pipeline.",
  "runtime": {
    "deno": true,
    "node": true,
    "workerd": true,
    "bun": true
  },
  "license": "MIT",
  "exports": "./mod.ts",
  "author": "Sean Knowles <s@seanknowles.dev>",
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 80,
    "semiColons": true,
    "singleQuote": false
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "publish": {
    "include": [
      "src/",
      "mod.ts",
      "README.md",
      "deno.json",
      "assets/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tellimer/open-source.git",
    "directory": "packages/econify"
  },
  "bugs": {
    "url": "https://github.com/Tellimer/open-source/issues"
  },
  "homepage": "https://github.com/Tellimer/open-source/tree/main/packages/econify#readme"
}
