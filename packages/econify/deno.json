{
  "name": "@tellimer/econify",
  "version": "1.3.2",
  "tasks": {
    "dev": "deno run --watch src/helpers/sample_usage.ts",
    "test": "deno test --parallel",
    "test:watch": "deno test --parallel --watch",
    "test:cov": "deno test --allow-read --coverage=coverage --parallel",
    "cov:lcov": "deno coverage coverage --lcov --exclude=\"src/io/|src/historical/|src/fx/live_fx.ts|src/inflation/inflation.ts|src/inference/inference.ts\" > coverage/lcov.info",
    "cov:check": "deno run --allow-read scripts/check_coverage.ts coverage/lcov.info 80",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "update-code3": "deno run --allow-read --allow-write src/helpers/update_code3.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "xstate": "npm:xstate@^5.20.2"
  },
  "description": "Economic data normalization and conversion toolkit: parse units, normalize values across currency, magnitude, and time with quality scoring, FX (live/historical), batch processing, and an XState-powered pipeline. For indicator classification, see @tellimer/classify.",
  "runtime": {
    "deno": true,
    "node": true,
    "workerd": true,
    "bun": true
  },
  "license": "MIT",
  "exports": "./mod.ts",
  "author": "Sean Knowles <s@seanknowles.dev>",
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 80,
    "semiColons": true,
    "singleQuote": false
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "publish": {
    "include": ["src/", "mod.ts", "README.md", "deno.json", "assets/"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tellimer/open-source.git",
    "directory": "packages/econify"
  },
  "bugs": {
    "url": "https://github.com/Tellimer/open-source/issues"
  },
  "homepage": "https://github.com/Tellimer/open-source/tree/main/packages/econify#readme"
}
