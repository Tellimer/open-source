{
  "name": "@tellimer/classify",
  "version": "0.1.0",
  "tasks": {
    "dev": "deno run --watch examples/basic_usage.ts",
    "test": "deno test --env --allow-env --allow-net --allow-read --allow-write --parallel",
    "test:unit": "deno test src/ --parallel",
    "test:int": "deno test --env --allow-env --allow-net --allow-read tests/e2e/classification_accuracy_test.ts",
    "test:v2": "deno test --env --allow-env --allow-net --allow-read --allow-write --allow-ffi tests/v2/",
    "test:watch": "deno test --env --allow-env --allow-net --allow-read --allow-write --parallel --watch",
    "test:cov": "deno test --env --allow-env --allow-net --allow-read --allow-write --coverage=coverage --parallel",
    "test:dry": "deno test tests/e2e/dry_run_test.ts",
    "cov:lcov": "deno coverage coverage --lcov > coverage/lcov.info",
    "bench": "deno bench --env --allow-env --allow-net tests/bench/classification_bench.ts",
    "cost-analysis": "deno run --env --allow-env --allow-net examples/cost_analysis.ts",
    "db:setup": "deno run --allow-all src/v2/db/setup.ts setup",
    "db:reset": "deno run --allow-all src/v2/db/setup.ts reset",
    "db:clean": "deno run --allow-all src/v2/db/setup.ts clean",
    "db:seed": "deno run --allow-all scripts/seed_database.ts",
    "db:generate-fixtures": "deno run --allow-all scripts/generate_test_fixture.ts",
    "prod:setup": "deno run --env --allow-all scripts/production/setup.ts",
    "prod:seed": "deno run --env --allow-all scripts/production/seed.ts",
    "prod:run": "deno run --env --allow-all scripts/production/run_pipeline.ts",
    "prod:backfill-orientation": "deno run --env --allow-all scripts/production/backfill_orientation_reasoning.ts",
    "prod:flag": "deno run --env --allow-all scripts/production/run_flagging.ts",
    "prod:review": "deno run --env --allow-all scripts/production/run_review.ts",
    "prod:sync": "deno run --env --allow-all scripts/production/sync_to_railway.ts",
    "prod:reset": "deno run --env --allow-all scripts/production/reset.ts",
    "prod:reset-local": "deno run --env --allow-all scripts/production/reset_local.ts",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "review:all": "deno run --env --allow-all src/v2/scripts/review_all.ts",
    "review:all-flag": "deno run --env --allow-all src/v2/scripts/review_all_flag.ts"
  },
  "imports": {
    "@db/sqlite": "jsr:@db/sqlite@^0.12.0",
    "@libsql/client": "npm:@libsql/client@^0.14.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.22",
    "@std/testing": "jsr:@std/testing@^1",
    "ai": "npm:ai@^5.0.59",
    "@ai-sdk/openai": "npm:@ai-sdk/openai@^2.0.0",
    "@ai-sdk/anthropic": "npm:@ai-sdk/anthropic@^2.0.22",
    "@ai-sdk/google": "npm:@ai-sdk/google@^1.0.8",
    "@ai-sdk/valibot": "npm:@ai-sdk/valibot@^1.0.10",
    "valibot": "npm:valibot@^1.1.0"
  },
  "description": "LLM-based economic indicator classification and metadata enrichment for TypeScript/Deno. Supports OpenAI, Anthropic, and Google Gemini.",
  "runtime": {
    "deno": true,
    "node": true,
    "workerd": true,
    "bun": true
  },
  "license": "MIT",
  "exports": "./mod.ts",
  "author": "Sean Knowles <s@seanknowles.dev>",
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 80,
    "semiColons": true,
    "singleQuote": false
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any",
        "prefer-const"
      ]
    }
  },
  "publish": {
    "include": [
      "src/",
      "mod.ts",
      "README.md",
      "deno.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tellimer/open-source.git",
    "directory": "packages/classify"
  },
  "bugs": {
    "url": "https://github.com/Tellimer/open-source/issues"
  },
  "homepage": "https://github.com/Tellimer/open-source/tree/main/packages/classify#readme"
}
